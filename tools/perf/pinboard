import logging
import sys

from core import path_util
path_util.AddPyUtilsToPath()

from cli_tools import pinboard


if __name__ == '__main__':
  try:
    sys.exit(pinboard.Main())
  except Exception:
    logging.exception('Terminated with exception:')
    sys.exit(1)
